{
  "name": "@scafflater/jest-scafflater",
  "version": "0.0.3",
  "description": "Jest Matchers to test generated templates",
  "main": "src/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test:dev": "node --experimental-vm-modules node_modules/.bin/jest --detectOpenHandles --collectCoverage --runInBand --verbose --forceExit",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --collectCoverage",
    "lint": "eslint \"**/*.js\" --ignore-pattern node_modules/",
    "lint:fix": "eslint --fix \"**/*.js\" --ignore-pattern node_modules/",
    "publish-package": "npm publish --access public",
    "update-readme-toc": "doctoc .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scafflater/jest-scafflater.git"
  },
  "keywords": [
    "scafflater",
    "jest"
  ],
  "author": "chicoribas (chico.bribas@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scafflater/jest-scafflater/issues"
  },
  "homepage": "https://github.com/scafflater/jest-scafflater#readme",
  "dependencies": {
    "dir-compare": "5.0.0",
    "fs-extra": "11.2.0",
    "jest": "29.7.0",
    "jest-matcher-utils": "29.7.0",
    "prettier": "3.3.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "doctoc": "2.2.1",
    "eslint": "8.57.0",
    "eslint-config-oclif": "5.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-jsdoc": "48.2.9",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.2.0",
    "husky": "9.0.11"
  }
}
